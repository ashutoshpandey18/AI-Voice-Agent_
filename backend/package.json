{
  "name": "voice-agent-backend",
  "version": "1.0.0",
  "description": "Backend API for AI Voice Agent Restaurant Booking System",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:clean": "node scripts/clean-port.js && npm run dev",
    "build": "tsc",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "clean-port": "node scripts/clean-port.js",
    "seed-admin": "node -e \"console.log('seed-admin disabled by default. Set ALLOW_SEED=true and run ts-node src/scripts/seedAdmin.ts')\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "voice-agent",
    "restaurant",
    "booking",
    "express",
    "typescript",
    "ai",
    "nlp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.0.3",
    "pdfkit": "^0.15.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/pdfkit": "^0.13.4",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
